datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Users {
  id       String   @id @default(uuid())
  name     String?  @default("")
  email    String   @unique
  username String   @unique
  password String
  updated  DateTime @updatedAt()
  created  DateTime @default(now())
}
